## SCANNING
nikto() {
    /usr/bin/nikto -host "${1}" | tee nikto."${1}".out
}

function gobust() {
    if [[ -z $2 ]]; then
        wordlist=/usr/share/dirbuster/wordlists/directory-list-2.3-medium.txt
    else
        wordlist=$2
    fi
    if [[ "${1}" == "-h" || "${1}" == "--help" ]]; then
        echo gobuster -u $1 -w $wordlist -s '200,204,301,302,307,403,500' -e php,aspx,txt,html -t 50 | tee gobuster.$1.out
    else
        gobuster -u $1 -w $wordlist -s '200,204,301,302,307,403,500' -e php,aspx,txt,html -t 50 | tee gobuster.$1.out
    fi
}

function dirsear() {
    if [[ -z $2 ]]; then
        wordlist=/usr/share/dirbuster/wordlists/directory-list-2.3-medium.txt
    else
        wordlist=$2
    fi
    if [[ "${1}" == "-h" || "${1}" == "--help" ]]; then
        echo dirsearch -u $1 -w $wordlist -e php,txt,html,aspx -t 150 | tee dirsearch$
    else
        dirsearch -u $1 -w $wordlist -e php,txt,html,aspx -t 150 | tee disearch.$1.o$
    fi
}

ms() {

   masscan -p1-65535,U:1-65535 "${1}" --rate=1000 -e tun0 > ports
}

ms-clean() {

   ports=$(cat ports | awk -F " " '{print $4}' | awk -F "/" '{print $1}' | sort -n | tr '\n' ',' | sed 's/,$//')
}

ms-nmap() {

   nmap -Pn -sV -sC -vv -p$ports "${1}" -oA nmap
}

adrive() {

   sshpass -p "password" scp "${1}" email@scp.adrive.com:.
}
